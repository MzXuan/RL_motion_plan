<?xml version="1.0"?>
<robot name="upper_body">

     <material name="red">
    <color rgba="0.8 0.0 0.0 0.5 "/>
  </material>
  <material name="green">
    <color rgba="0.0 0.8 0.0 0.5 "/>
  </material>




	<!-- Base Frame of human, spine -->
	<link name="world"/>


	<link name="SpineBase">
		<visual>
			<geometry>
				<sphere radius="0.05"/>
			</geometry>
		</visual>
	</link>


	<joint name="base_spine_joint" type="fixed">
		<parent link="world"/>
		<child link="SpineBase"/>
		<origin rpy="0 0 0" xyz="0 0 0"/>
	</joint>


	<link name="SpineLink">
		<visual>
			<geometry>
				<cylinder radius="0.08" length="0.4"/>
			</geometry>
		</visual>
	</link>

	<joint name="spine_joint" type="fixed">
		<parent link="SpineBase"/>
		<child link="SpineLink"/>
		<origin rpy="1.57 0 0.0" xyz="0 0.2 0"/>
	</joint>



	<joint name="shoulder_spine_joint" type="fixed">
		<parent link="SpineLink"/>
		<child link="shoulder_spine_link"/>
		<origin rpy="-1.57 0 0.0" xyz="0 0 -0.2"/>
	</joint>


	<link name="shoulder_spine_link">
		<visual>
			<geometry>
				<sphere radius="0.05"/>
			</geometry>
		</visual>
	</link>


	<!-- HEAD  -->
	<link name="head_link">
		<visual>
			<geometry>
				<sphere radius="0.07"/>
			</geometry>
		</visual>
	</link>

	<joint name="fix_head_joint" type="fixed">
		<parent link="shoulder_spine_link"/>
		<child link="head_link"/>
		<origin rpy="0.0 0.0 0.0" xyz="0.0 0.1 0"/>
	</joint>



	<!-- LEFT arm -->

	<!-- shoulder spine -->
	<link name="left_spine_theta_link" />

	<joint name="LShoulderSY" type="continuous">
		<parent link="shoulder_spine_link"/>
		<child link="left_spine_theta_link" />
		<axis xyz="0 1 0"/>
		<dynamics damping="0.005" />
	</joint>

	
	<link name="left_spine_phi_link" />
	<joint name="LShoulderSZ" type="continuous">
		<parent link="left_spine_theta_link"/>
		<child link="left_spine_phi_link" />
		<axis xyz="0 0 1"/>
		<dynamics damping="0.005" />
	</joint>

	
	<joint name="left_shoulder_fixed" type="fixed">
		<parent link="left_spine_phi_link"/>
		<child link="left_spine_to_shoulde_link" />
		<origin rpy="0 1.57 0" xyz="0.08 0 0"/>
		
	</joint>	


	<link name="left_spine_to_shoulde_link">
		<visual>
			<geometry>
				<cylinder radius="0.02" length="0.16"/>
			</geometry>
		</visual>
	</link>


	<!-- shoulder  -->
	<link name="left_shoulder_link">
		<visual>
			<geometry>
				<sphere radius="0.02"/>
			</geometry>
		</visual>
	</link>



	<joint name="LShoulderX" type="continuous">
		<parent link="left_spine_to_shoulde_link"/>
		<child link="left_shoulder_link"/>
		<axis xyz="1 0 0" />
		<origin rpy="1.57 0 -1.57" xyz="0 0 0.08"/>
	</joint>


	<link name="left_shoulder_theta_link" />
	<joint name="LShoulderY" type="continuous" >
	    <parent link="left_shoulder_link"/>
	    <child link="left_shoulder_theta_link"/>
	    <axis xyz="0 1 0" />
	    <dynamics damping="0.005" />
	</joint>


	<joint name="LShoulderZ" type="continuous" >
	    <parent link="left_shoulder_theta_link"/>
	    <child link="ShoulderLeft"/>
	    <axis xyz="0 0 1" />
	    <dynamics damping="0.005" />
	</joint>

	<link name="ShoulderLeft"/>
	<link name="left_upper_arm">
		<visual>
			<geometry>
				<cylinder length="0.22" radius="0.03"/>
			</geometry>
		</visual>
	</link>
	<joint name="left_should_fixed" type="fixed" >
	    <parent link="ShoulderLeft"/>
	    <child link="left_upper_arm"/>
	    <origin rpy="0 1.57 0" xyz="0.11 0 0.0"/>
	    <dynamics damping="0.005" />
	</joint>


<!-- elbow -->
	<link name="left_elbow">
		<visual>
			<geometry>
				<sphere radius="0.02"/>
			</geometry>
		</visual>
	</link>

	<joint name="left_fix_elbow_joint" type="fixed">
		<parent link="left_upper_arm"/>
		<child link="left_elbow"/>
		<origin rpy="1.57 0 3.14" xyz="0 0.0 0.11"/>
	</joint>


	<link name="left_lower_arm">
		<visual>
			<geometry>
				<cylinder length="0.28" radius="0.02"/>
			</geometry>
		</visual>
	</link>

	<joint name="LElbowX" type="continuous" >
	    <parent link="left_elbow" />
	    <child link="left_elbow_psi_link" />
	    <axis xyz="1 0 0" />
	    <dynamics damping="0.005" />
	</joint>

	<link name="left_elbow_psi_link" />

	<joint name="LElbowY" type="continuous" >
	    <parent link="left_elbow_psi_link"/>
	    <child link="left_elbow_theta_link"/>
	    <axis xyz="0 1 0" />
	    <dynamics damping="0.005" />
	</joint>

	<link name="left_elbow_theta_link" />

	<joint name="LElbowZ" type="continuous" >
	    <parent link="left_elbow_theta_link"/>
	    <child link="ElbowLeft"/>
	    <axis xyz="0 0 1" />
	    <dynamics damping="0.005" />
	</joint>


	<link name="ElbowLeft"/>
	<joint name="left_elbow_fixed3" type="fixed" >
	    <parent link="ElbowLeft"/>
	    <child link="left_lower_arm"/>
	    <origin rpy="1.57 0 0.0" xyz="0 0.11 0"/>
	    <dynamics damping="0.005" />
	</joint>

	<link name="left_hand">
		<visual>
			<geometry>
				<sphere radius="0.05"/>
			</geometry>
		<material name="red"/>
		</visual>
	</link>
	<joint name="left_hand_fixed" type="fixed" >
	    <parent link="left_lower_arm"/>
	    <child link="left_hand"/>
	    <origin rpy="0 0 0.0" xyz="0 0 -0.11"/>
	    <dynamics damping="0.005" />
	</joint>


<!-- RIGHT ARM -->
	<!-- shoulder spine -->
	<link name="right_spine_theta_link" />

	<joint name="RShoulderSY" type="continuous">
		<parent link="shoulder_spine_link"/>
		<child link="right_spine_theta_link" />
		<axis xyz="0 1 0"/>
		<dynamics damping="0.005" />
	</joint>

	
	<link name="right_spine_phi_link" />
	<joint name="RShoulderSZ" type="continuous">
		<parent link="right_spine_theta_link"/>
		<child link="right_spine_phi_link" />
		<axis xyz="0 0 1"/>
		<dynamics damping="0.005" />
	</joint>

	
	<joint name="right_shoulder_fixed" type="fixed">
		<parent link="right_spine_phi_link"/>
		<child link="right_spine_to_shoulde_link" />
		<origin rpy="0 1.57 0" xyz="-0.08 0 0"/>
		
	</joint>	


	<link name="right_spine_to_shoulde_link">
		<visual>
			<geometry>
				<cylinder radius="0.02" length="0.16"/>
			</geometry>
		</visual>
	</link>


	<!-- shoulder  -->
	<link name="right_shoulder_link">
		<visual>
			<geometry>
				<sphere radius="0.02"/>
			</geometry>
		</visual>
	</link>



	<joint name="RShoulderX" type="continuous">
		<parent link="right_spine_to_shoulde_link"/>
		<child link="right_shoulder_link"/>
		<axis xyz="1 0 0" />
		<origin rpy="-1.57 0 1.57" xyz="0 0 -0.08"/>
	</joint>


	<link name="right_shoulder_theta_link" />
	<joint name="RShoulderY" type="continuous" >
	    <parent link="right_shoulder_link"/>
	    <child link="right_shoulder_theta_link"/>
	    <axis xyz="0 1 0" />
	    <dynamics damping="0.005" />
	</joint>


	<joint name="RShoulderZ" type="continuous" >
	    <parent link="right_shoulder_theta_link"/>
	    <child link="ShoulderRight"/>
	    <axis xyz="0 0 1" />
	    <dynamics damping="0.005" />
	</joint>

	<link name="ShoulderRight"/>
	<link name="right_upper_arm">
		<visual>
			<geometry>
				<cylinder length="0.22" radius="0.03"/>
			</geometry>
		</visual>
	</link>
	<joint name="right_should_fixed" type="fixed" >
	    <parent link="ShoulderRight"/>
	    <child link="right_upper_arm"/>
	    <origin rpy="0 1.57 0" xyz="-0.11 0 0.0"/>
	    <dynamics damping="0.005" />
	</joint>

<!-- elbow -->
	<link name="right_elbow">
		<visual>
			<geometry>
				<sphere radius="0.02"/>
			</geometry>
		</visual>
	</link>

	<joint name="right_fix_elbow_joint" type="fixed">
		<parent link="right_upper_arm"/>
		<child link="right_elbow"/>
		<origin rpy="-1.57 0 0" xyz="0 0.0 -0.11"/>
	</joint>




	<joint name="RElbowX" type="continuous" >
	    <parent link="right_elbow" />
	    <child link="right_elbow_psi_link" />
	    <axis xyz="1 0 0" />
	    <dynamics damping="0.005" />
	</joint>

	<link name="right_elbow_psi_link" />

	<joint name="RElbowY" type="continuous" >
	    <parent link="right_elbow_psi_link"/>
	    <child link="right_elbow_theta_link"/>
	    <axis xyz="0 1 0" />
	    <dynamics damping="0.005" />
	</joint>

	<link name="right_elbow_theta_link" />

	<joint name="RElbowZ" type="continuous" >
	    <parent link="right_elbow_theta_link"/>
	    <child link="ElbowRight"/>
	    <axis xyz="0 0 1" />
	    <dynamics damping="0.005" />
	</joint>


	<link name="ElbowRight"/>
	<joint name="right_elbow_fixed3" type="fixed" >
	    <parent link="ElbowRight"/>
	    <child link="right_lower_arm"/>
	    <origin rpy="1.57 0 0.0" xyz="0 0.11 0"/>
	    <dynamics damping="0.005" />
	</joint>

	<link name="right_lower_arm">
		<visual>
			<geometry>
				<cylinder length="0.28" radius="0.02"/>
			</geometry>
		</visual>
	</link>

	<link name="right_hand">
		<visual>
			<geometry>
				<sphere radius="0.05"/>
			</geometry>
		<material name="red"/>
		</visual>
	</link>
	<joint name="right_hand_fixed" type="fixed" >
	    <parent link="right_lower_arm"/>
	    <child link="right_hand"/>
	    <origin rpy="0 0 0.0" xyz="0 0 -0.11"/>
	    <dynamics damping="0.005" />
	</joint>


</robot>